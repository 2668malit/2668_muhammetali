import cv2

image = 255 * np.ones(shape=[512, 512, 3], dtype=np.uint8)

cv2.line(image, (0, 0), (511, 511), (255, 0, 0), 5)

cv2.rectangle(image, (100, 100), (300, 300), (0, 255, 0), 3)

cv2.circle(image, (256, 256), 100, (0, 0, 255), -1)

cv2.imshow('Ã‡izimler', image)
